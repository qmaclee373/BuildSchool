!function(t){"use strict";var a={init:function(){a.layoutPanel()},layoutPanel:function(){var a=t(".isotope"),o=t(".scroll-wrap"),e=t("[data-custom-toggle]");e[0]&&e.each(function(){var a=t(this);a.on("click",t.proxy(function(a){a.preventDefault();var o=t(this);t(o.attr("data-custom-toggle")).add(this).toggleClass("active")},a)),"true"===a.attr("data-custom-toggle-hide-on-blur")&&t("body").on("click",a,function(a){a.target!==a.data[0]&&t(a.data.attr("data-custom-toggle")).find(t(a.target)).length&&0===a.data.find(t(a.target)).length&&t(a.data.attr("data-custom-toggle")).add(a.data[0]).removeClass("active")}),"true"===a.attr("data-custom-toggle-disable-on-blur")&&t("body").on("click",a,function(a){a.target!==a.data[0]&&0===t(a.data.attr("data-custom-toggle")).find(t(a.target)).length&&0===a.data.find(t(a.target)).length&&t(a.data.attr("data-custom-toggle")).add(a.data[0]).removeClass("active")})}),o[0]&&o.each(function(){var a=t(this);a.mCustomScrollbar({theme:a.attr("data-theme")?a.attr("data-theme"):"minimal",scrollInertia:100,scrollButtons:{enable:!1}})}),a[0]&&a.each(function(){var a=t(this),o=a.closest(".isotope-wrap").find("[data-isotope-filter]");a.isotope({itemSelector:".isotope-item",layoutMode:a.attr("data-isotope-layout")?a.attr("data-isotope-layout"):"fitRows",filter:"*"}),a.on("arrangeComplete",function(){t(this).addClass("isotope--loaded")}),o.on("click",function(a){a.preventDefault();var o=t(this),e=t('.isotope[data-isotope-group="'+o.attr("data-isotope-group")+'"]'),i=o.closest(".isotope-filters");i.find(".active").removeClass("active"),o.addClass("active"),e.removeClass("isotope--loaded"),e.isotope({itemSelector:".isotope-item",layoutMode:e.attr("data-isotope-layout")?e.attr("data-isotope-layout"):"fitRows",filter:"*"===o.attr("data-isotope-filter")?"*":'[data-filter*="'+o.attr("data-isotope-filter")+'"]'}),e.on("arrangeComplete",function(){t(this).addClass("isotope--loaded")})}).eq(0).trigger("click")})}};t(document).on("ready",a.init)}(jQuery);
